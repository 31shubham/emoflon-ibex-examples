#using SokobanImportExport.*
#using AttrCondDefLibrary.*

#abstract #rule EntryToFieldRule #with SokobanImportExport

#source { 
	e:Entry 
	++ ne:Entry	
}

#target {
	f:Field 
	++ nf:Field
	
	board:Board {
		-fields->f
		++ -fields->nf
	}	
}

#correspondence {
	l2lf:EntryToField {
		#src->e
		#trg->f
	}
	
	++ r2rf:EntryToField {
		#src->ne
		#trg->nf
	}
}
