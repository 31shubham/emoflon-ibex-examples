#using SokobanImportExport.*
#using AttrCondDefLibrary.*

#abstract #rule RowRule 
#extends EntryToFieldRule, NextLinkRule 
#with SokobanImportExport

#source { 
	e:Entry
	
	++ ne:Entry

	r:Row {
		-firstEntry->e
		++ -next->nr
	}
		
	++ nr:Row {
		++ -firstEntry->ne
	}	
}

#target {
	f:Field{
		++ -bottom->nf
	}
	
	++ nf:Field
}

#attributeConditions {
	add(f.row, 1, nf.row)
	eq_int(f.col, nf.col)
}
