#using SokobanImportExport.*
#using AttrCondDefLibrary.*

#rule LinkToBottomRule #extends NextLinkRule #with SokobanImportExport

#source { 
	e:Entry
	ne:Entry
}

#target {
	f:Field {
		++ -bottom->nf
	}
	
	nf:Field
}

#correspondence {
	l2lf:EntryToField {
		#src->e
		#trg->f
	}
	
	r2rf:EntryToField {
		#src->ne
		#trg->nf
	}
}
