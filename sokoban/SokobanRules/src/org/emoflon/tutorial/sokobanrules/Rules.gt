import "platform:/resource/SokobanLanguage/model/SokobanLanguage.ecore"

pattern moreThanOneSokoban() {
	sokoban: Sokoban

	otherSokoban: Sokoban
}

pattern oneSokoban() {
	sokoban: Sokoban
}

pattern oneEndField() {
	field: Field {
		.endPos == true
	}
}

pattern anOccupiedEndField() {
	fig: Figure

	field: Field {
		.endPos == true
		-figure -> fig
	}
}

pattern oneBlock() {
	block: Block
}
