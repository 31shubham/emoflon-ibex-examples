import "platform:/resource/SokobanLanguage/model/SokobanLanguage.ecore"

rule moreThanOneSokoban() {
	sokoban:Sokoban 
	otherSokoban:Sokoban
}

rule oneSokoban(){
	sokoban:Sokoban
}

rule oneEndField(){
	field:Field {
		.endPos == true
	}
}

rule anOccupiedEndField(){
	fig:Figure
	
	field:Field {
		.endPos == true
		-figure->fig
	}
}

rule oneBlock(){
	block:Block
}