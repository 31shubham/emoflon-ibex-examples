        <attribute href="platform:/resource/FacebookLanguage/model/FacebookLanguage.ecore#//User/name"/>
      </values>
      <values xsi:type="tgg:AttributeExpression" objectVar="//@rules.2/@targetPatterns.0">
        <attribute href="platform:/resource/InstagramLanguage/model/InstagramLanguage.ecore#//User/name"/>
