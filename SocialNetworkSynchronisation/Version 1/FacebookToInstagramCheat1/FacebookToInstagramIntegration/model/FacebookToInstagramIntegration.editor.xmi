        <value>B</value>
        <value>B</value>
      </allowedGenAdornments>
      <allowedGenAdornments>
        <value>B</value>
        <value>F</value>
      </allowedGenAdornments>
      <allowedGenAdornments>
        <value>F</value>
        <value>B</value>
      </allowedGenAdornments>
      <allowedGenAdornments>
        <value>F</value>
        <value>F</value>
      </allowedGenAdornments>
