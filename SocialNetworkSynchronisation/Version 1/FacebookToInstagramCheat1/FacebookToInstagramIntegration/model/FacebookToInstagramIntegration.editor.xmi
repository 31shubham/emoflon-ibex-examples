    <correspondencePatterns name="f2i" type="//@schema/@correspondenceTypes.1" source="//@rules.0/@sourcePatterns.0" target="//@rules.0/@targetPatterns.0"/>
    <correspondencePatterns name="fu2iu" type="//@schema/@correspondenceTypes.0" source="//@rules.0/@sourcePatterns.1" target="//@rules.0/@targetPatterns.1"/>
  </rules>
  <rules name="FacebookToInstagram" schema="//@schema">
