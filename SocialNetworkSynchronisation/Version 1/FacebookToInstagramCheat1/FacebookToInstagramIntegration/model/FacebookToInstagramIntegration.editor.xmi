        <value>B</value>
      </allowedSyncAdornments>
      <allowedSyncAdornments>
        <value>B</value>
        <value>F</value>
      </allowedSyncAdornments>
      <allowedSyncAdornments>
        <value>F</value>
        <value>B</value>
